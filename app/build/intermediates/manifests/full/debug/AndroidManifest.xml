        </activity>
        <activity android:name="setiaendra18.gmail.com.akakommobile.MainActivity" />
        <activity android:name="setiaendra18.gmail.com.akakommobile.profil" />
        <activity android:name="setiaendra18.gmail.com.akakommobile.program_studi" />
        <activity android:name="setiaendra18.gmail.com.akakommobile.tautan" />
